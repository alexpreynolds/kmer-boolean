#!/usr/bin/env Rscript

d <- read.table("hg38.txt", sep="\t", header=T)
print(d)

require(ggplot2)
require(scales)

out_fn <- 'hg38.pdf'
pdf(out_fn, onefile=F, width=7, height=5)
p <- ggplot(d, aes(x=k, y=absent)) +
     geom_point() +
     scale_y_log10(breaks = trans_breaks("log10", function(x) 10^x), labels = trans_format("log10", math_format(10^.x))) +
     theme_bw()
print(p)
dev.off()